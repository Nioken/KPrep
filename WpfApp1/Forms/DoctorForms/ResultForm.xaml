<Window x:Class="WpfApp1.Forms.DoctorForms.ResultForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1.Forms.DoctorForms" xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="ResultForm" Height="635.536" Width="843.3" Loaded="Window_Loaded" Icon="/WpfApp1;component/Assets/Images/icon2.png">
    <Grid>
        <StackPanel Margin="10">
            <Label x:Name="InfoLabel" FontSize="16" Content="Text"/>
            <Label x:Name="ResLabel" Content="Результат:" FontSize="14"/>
            <xctk:RichTextBox x:Name="ResultBox" Height="494">
                <xctk:RichTextBoxFormatBarManager.FormatBar>
                    <xctk:RichTextBoxFormatBar/>
                </xctk:RichTextBoxFormatBarManager.FormatBar>
            </xctk:RichTextBox>
            <WrapPanel>
                <Button Content="Сохранить" Margin="0,5,0,0" Click="Button_Click"/>

            </WrapPanel>
        </StackPanel>
    </Grid>
</Window>
